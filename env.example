DATA_MODE=mock
CAN_IFACE=can0
WS_PORT=8080
HTTP_PORT=3000
DB_PATH=./data/telemetry.db
REPLAY_FILE=./samples/replay.json
DBC_JSON=./dbc/vehicle.json
LOG_LEVEL=info
PERFORMANCE_MODE=normal
ENABLE_RAW_FRAME_LOG=false
DIAGNOSTICS_ENABLED=true
HEALTH_CHECK_INTERVAL=30

# ROS Configuration
# rosbridge WebSocket URL (default: ws://localhost:9090)
VITE_ROS_BRIDGE_URL=ws://localhost:9090
# Map frame name (default: map)
VITE_MAP_FRAME=map

# Map tile configuration
# Primary tile service base URL (e.g. http://your-host:8000/maptiles/)
VITE_MAP_TILE_SERVICE_URL=
# Optional JSON array string with extra tile sources; see docs/map-configuration.md
VITE_MAP_TILE_SOURCES=
# Map coordinate scale (map units to meters). Leave empty to use default 1.0
VITE_MAP_METERS_PER_UNIT=
# Preferred tile source id (must match one from configuration)
VITE_MAP_DEFAULT_TILE_SOURCE=

